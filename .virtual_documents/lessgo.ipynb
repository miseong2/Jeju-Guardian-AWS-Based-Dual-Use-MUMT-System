# 2ë²ˆ ëª¨ë¸(Seed 2) URL
url = "https://marinedebrisdetector.s3.eu-central-1.amazonaws.com/checkpoints/unet%2B%2B2/epoch=95-val_loss=0.65-auroc=0.988.ckpt"
