# yoloìš© ë°ì´í„°ì…‹ ìƒì„± ì½”ë“œ
import os
import rasterio
import numpy as np
import torch
import cv2
from tqdm import tqdm # ì§„í–‰ë¥  í‘œì‹œë°” (ì„¤ì¹˜ ì•ˆë˜ì–´ìˆìœ¼ë©´ !pip install tqdm)

# --- [ìµœì¢… ì„¤ì •] ---
CONF_THRESHOLD = 0.1   # ë¯¸ì„±ë‹˜ì´ ë§Œì¡±í•œ ê¸°ì¤€ (0.1)
BRIGHTNESS_FACTOR = 5.0 # ì´ë¯¸ì§€ë¥¼ 5ë°° ë°ê²Œ ì €ì¥
BASE_DIR = "MARIDA/patches"
OUTPUT_DIR = "dataset_yolo"

# í´ë” ìƒì„± (ì—†ìœ¼ë©´ ë§Œë“¦)
os.makedirs(f"{OUTPUT_DIR}/images", exist_ok=True)
os.makedirs(f"{OUTPUT_DIR}/labels", exist_ok=True)

# --- YOLO ì¢Œí‘œ ë³€í™˜ í•¨ìˆ˜ ---
def convert_to_yolo_format(bbox, img_w, img_h):
    # bbox: (x_min, y_min, w, h)
    # YOLO: (x_center, y_center, w_norm, h_norm) - 0~1 ì •ê·œí™”
    x, y, w, h = bbox
    
    x_center = (x + w / 2) / img_w
    y_center = (y + h / 2) / img_h
    w_norm = w / img_w
    h_norm = h / img_h
    
    return x_center, y_center, w_norm, h_norm

# --- ê³µì¥ ê°€ë™ í•¨ìˆ˜ ---
def generate_yolo_dataset():
    # ëª¨ë¸ì´ ë¡œë“œë˜ì–´ ìˆëŠ”ì§€ í™•ì¸
    if 'model' not in globals():
        print("âŒ ì˜¤ë¥˜: 'model'ì´ ë¡œë“œë˜ì§€ ì•Šì•˜ìŠµë‹ˆë‹¤. ëª¨ë¸ ë¡œë“œ ì½”ë“œë¥¼ ë¨¼ì € ì‹¤í–‰í•´ì£¼ì„¸ìš”!")
        return

    print(f"ğŸ­ YOLO ë°ì´í„°ì…‹ ê³µì¥ ê°€ë™ ì‹œì‘! (ê¸°ì¤€: {CONF_THRESHOLD}, ë°ê¸°: x{BRIGHTNESS_FACTOR})")
    print(f"ğŸ“‚ ëŒ€ìƒ í´ë”: {BASE_DIR}")
    
    # 1. ëª¨ë“  í´ë” íƒìƒ‰
    if not os.path.exists(BASE_DIR):
        print(f"âŒ ë°ì´í„° í´ë”ë¥¼ ì°¾ì„ ìˆ˜ ì—†ìŠµë‹ˆë‹¤: {BASE_DIR}")
        return

    subfolders = [f for f in os.listdir(BASE_DIR) if os.path.isdir(os.path.join(BASE_DIR, f)) and not f.startswith('.')]
    
    generated_count = 0
    skipped_count = 0
    
    # tqdmìœ¼ë¡œ ì§„í–‰ ìƒí™© ë³´ì—¬ì¤Œ
    for folder in tqdm(subfolders, desc="Processing Folders"):
        folder_path = os.path.join(BASE_DIR, folder)
        
        # _cl.tif íŒŒì¼ë“¤ë§Œ ì°¾ê¸°
        label_files = [f for f in os.listdir(folder_path) if f.endswith('_cl.tif')]
        
        for lbl_file in label_files:
            img_file = lbl_file.replace('_cl.tif', '.tif')
            img_path = os.path.join(folder_path, img_file)
            lbl_path = os.path.join(folder_path, lbl_file)
            
            # ì´ë¯¸ì§€ê°€ ì—†ìœ¼ë©´ ìŠ¤í‚µ
            if not os.path.exists(img_path): continue
            
            try:
                # [A] ë°ì´í„° ë¡œë“œ (11ì±„ë„ -> 12ì±„ë„ íŒ¨ë”©)
                with rasterio.open(img_path) as src:
                    img = src.read()
                    height, width = src.height, src.width
                    img_tensor = torch.from_numpy(img).unsqueeze(0)
                    if img_tensor.shape[1] == 11:
                        dummy = torch.zeros((1, 1, height, width))
                        img_tensor = torch.cat([img_tensor, dummy], dim=1)
                
                with rasterio.open(lbl_path) as src:
                    gt_label = src.read(1)

                # [B] U-Net ì¶”ë¡ 
                with torch.no_grad():
                    output = model(img_tensor)
                    pred = torch.sigmoid(output).squeeze().numpy()

                # [C] ë°•ìŠ¤ ì¶”ì¶œ
                binary_mask = (pred > CONF_THRESHOLD).astype(np.uint8)
                contours, _ = cv2.findContours(binary_mask, cv2.RETR_EXTERNAL, cv2.CHAIN_APPROX_SIMPLE)
                
                yolo_labels = [] 
                
                for cnt in contours:
                    x, y, w, h = cv2.boundingRect(cnt)
                    if w < 2 or h < 2: continue
                    
                    # [D] ì •ë‹µì§€ ê²€ì¦ (Mapping)
                    roi = gt_label[y:y+h, x:x+w]
                    
                    class_id = -1
                    # â˜… YOLO í´ë˜ìŠ¤ ì •ì˜: 0=ì“°ë ˆê¸°, 1=ì„ ë°•
                    if np.sum(roi == 1) > 0: class_id = 0    # Debris
                    elif np.sum(roi == 5) > 0: class_id = 1  # Ship
                    
                    if class_id != -1:
                        xc, yc, wn, hn = convert_to_yolo_format((x, y, w, h), width, height)
                        # YOLO í¬ë§·: class x_center y_center width height
                        yolo_labels.append(f"{class_id} {xc:.6f} {yc:.6f} {wn:.6f} {hn:.6f}")

                # [E] ì €ì¥ (ìœ íš¨ ê°ì²´ê°€ ìˆëŠ” ê²½ìš°ì—ë§Œ)
                if yolo_labels:
                    # 1. ì´ë¯¸ì§€ ì €ì¥ (5ë°° ë°ê²Œ ë³´ì • -> JPG)
                    rgb = img_tensor.squeeze().numpy()[[3, 2, 1], :, :].transpose(1, 2, 0)
                    # ë°ê¸° 5ë°°, 0~1 í´ë¦¬í•‘, 255 ê³±í•˜ê¸°
                    rgb_enhanced = np.clip(rgb * BRIGHTNESS_FACTOR, 0, 1) * 255.0
                    rgb_enhanced = rgb_enhanced.astype(np.uint8)
                    
                    # RGB -> BGR (OpenCVìš©)
                    bgr_save = cv2.cvtColor(rgb_enhanced, cv2.COLOR_RGB2BGR)
                    
                    save_base_name = f"{folder}_{img_file.replace('.tif', '')}"
                    
                    # ì´ë¯¸ì§€ ì“°ê¸°
                    cv2.imwrite(f"{OUTPUT_DIR}/images/{save_base_name}.jpg", bgr_save)
                    
                    # ë¼ë²¨ ì“°ê¸°
                    with open(f"{OUTPUT_DIR}/labels/{save_base_name}.txt", "w") as f:
                        f.write("\n".join(yolo_labels))
                        
                    generated_count += 1
                else:
                    skipped_count += 1
                    
            except Exception as e:
                # ì—ëŸ¬ íŒŒì¼ì€ ê±´ë„ˆëœ€
                continue

    print("\n" + "="*50)
    print(f"ğŸ‰ ê³µì¥ ê°€ë™ ì™„ë£Œ!")
    print(f"âœ… ìƒì„±ëœ ë°ì´í„°ì…‹: {generated_count}ê°œ (ì´ë¯¸ì§€ + ë¼ë²¨)")
    print(f"ğŸ’¨ ê±´ë„ˆë›´ ë°ì´í„°(ë¹ˆ ë°”ë‹¤ ë“±): {skipped_count}ê°œ")
    print(f"ğŸ“‚ ì €ì¥ ìœ„ì¹˜: {OUTPUT_DIR}")
    print("="*50)

# ì‹¤í–‰!
generate_yolo_dataset()


